<% @prototypes.each do |prototype| %>
  <div class="card">
    <%= link_to image_tag(prototype.image, class: :card__img), prototype_path(prototype.id) %>
    <div class="card__body">
      <%= link_to prototype.titile, prototype_path(prototype.id), class: :card__title %>
      <p class="card__summary">
        <%= prototype.catch_copy %>
        <%= link_to "by #{protype.user.name}", user_path(prototype.user), class: :card__user %>
      </p>
    </div>
  </div>
<% end %>